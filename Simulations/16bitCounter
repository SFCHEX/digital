onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /stopwatch/clk_in
add wave -noupdate /stopwatch/cs/out_clock
add wave -noupdate /stopwatch/REVERSE
add wave -noupdate /stopwatch/START
add wave -noupdate /stopwatch/RESET
